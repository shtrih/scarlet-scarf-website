{
  "devDependencies": {
    "apache-server-configs": "2.11.0",
    "archiver": "^0.13.0",
    "del": "^1.1.0",
    "glob": "^4.2.1",
    "gulp": "^3.8.10",
    "gulp-header": "^1.2.2",
    "gulp-jscs": "^1.3.1",
    "gulp-jshint": "^1.9.0",
    "gulp-load-plugins": "^0.8.0",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.0",
    "jquery": "2.1.3",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.0.1",
    "normalize.css": "3.0.2",
    "run-sequence": "^1.0.2",
    "jquery.scrollto": "~2.1.0",
    "jquery.localscroll": "~1.4.0",
    "jquery.stellar": "git://github.com/shtrih/stellar.js#55aac27",
    "jquery.easing": "~1.3.1",
    "browserify": "~9.0.3",
    "vinyl-source-stream": "~1.1.0",
    "browserify-shim": "~3.8.3",
    "gulp-uglify": "~1.1.0",
    "vinyl-buffer": "~1.0.0",
    "gulp-sourcemaps": "~1.5.1",
    "gulp-util": "~3.0.4",
    "gulp-imagemin": "~2.2.1",
    "gulp-minify-css": "~1.0.0",
    "gulp-autoprefixer": "~2.1.0",
    "gulp-tinypng": "~1.0.2",
    "gulp-plumber": "~1.0.0",
    "skrollr": "git://github.com/Prinzhorn/skrollr#f09820f5"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "h5bp-configs": {
    "directories": {
      "archive": "archive",
      "dist": "dist",
      "src": "src",
      "test": "test"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "scrollto": "./node_modules/jquery.scrollto/jquery.scrollTo.min.js",
    "localscroll": "./node_modules/jquery.localscroll/jquery.localScroll.min.js",
    "stellar": "./node_modules/jquery.stellar/jquery.stellar.js",
    "easing": "./node_modules/jquery.easing/jquery.easing.1.3.min.js",
    "plugins": "./src/js/plugins.js",
    "anoslide": "./src/js/vendor/anoslide/js/jquery.anoslide.js",
    "main": "./src/js/main.js"
  },
  "browserify-shim": {
    "easing": {
      "depends": "jquery:jQuery"
    },
    "localscroll": {
      "depends": [
        "scrollto",
        "easing"
      ]
    },
    "main": {
      "exports": "main",
      "depends": [
        "jquery:$",
        "localscroll",
        "anoslide",
        "skrollr:skrollr",
        "plugins"
      ]
    }
  },
  "homepage": "http://h5bp.com/",
  "license": {
    "type": "MIT",
    "url": "https://github.com/h5bp/html5-boilerplate/blob/master/LICENSE.md"
  },
  "name": "html5-boilerplate",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "test": "gulp archive && mocha --reporter spec --timeout 5000"
  },
  "version": "5.0.0"
}
